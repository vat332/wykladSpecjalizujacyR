library(gmodels)
n = nrow(mydata)
labels <- t1[,1]
train = sample(1:n, size = round(0.7*n),replace = FALSE)
t1 = mydata[train,]
t2 = mydata[-train,]
n1 = nrow(t1)
n2 = nrow(t2)
library(class)
labels <- t1[,1]
error.rate <- numeric(10)
for(i in 1:10)
{
knn.pred <- knn(t1[,-1],t2[,-1],labels, k = i)
error.rate[i] <- 1-mean(knn.pred == t2[,1])
}
plot(1:10, error.rate,"b", pch = 20, col = "red", xlab = "K", ylab = "Error Rate")
