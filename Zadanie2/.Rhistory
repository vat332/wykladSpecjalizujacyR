library(gmodels)
n = nrow(mydata)
labels <- mydata[1:6001 ,1]
train = sample(1:n, size = round(0.7*n),replace = FALSE)
t1 = mydata[train,]
t2 = mydata[-train,]
print(t1)
print(t2)
n1 = nrow(t1)
n2 = nrow(t2)
labels <- t1[,1]
error.rate <- numeric(10)
for(i in 1:10)
{
knn.pred <- knn(t1[,-1],t2[,-1],labels, k = i)
error.rate[i] <- 1-mean(knn.pred == t2[,1])
}
